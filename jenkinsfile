pipeline{
  agent any
    stages{
      stage('Download'){
        steps{
          echo "continuous download"
        }
      }
      stage('Build'){
        steps{
          echo "continuous build"
        }
      }
      stage('Test'){
        steps{
          echo "continuous test"
        }
      }
      stage('Deploy'){
        steps{
          echo "continuous deploy"
        }
      }
      stage('Delivery'){
        steps{
          echo "continuous delivery"
        }
       }
    }
}
